import java.net.URL;

public class ParentWikipedia extends Wikipedia {

	private RelaventLinks relaventLinks;
	
	public ParentWikipedia(String pageTitle) {
		super(pageTitle);
		relaventLinks = new RelaventLinks(super.getURL());
	}
}


